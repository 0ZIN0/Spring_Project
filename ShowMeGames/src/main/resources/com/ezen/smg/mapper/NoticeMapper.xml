<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.ezen.smg.mapper.NoticeMapper">
 	<select id="getNotices" parameterType="int" resultType="com.ezen.smg.dto.NoticeDTO">
 		SELECT * FROM (SELECT rownum AS rn, a.* FROM notices a order by notice_id desc) WHERE rn BETWEEN 1 AND 10
 	</select>
 </mapper>